{
	"platform": {
	  "apiRuntime": "node:18"
	},
	"routes": [
	  {
		"route": "/api/*",
		"allowedRoles": ["anonymous"]
	  },
	  {
		"route": "/*",
		"serve": "/index.html",
		"statusCode": 200
	  }
	],
	"navigationFallback": {
	  "rewrite": "/index.html",
	  "exclude": ["/api/*"]
	},
	"responseOverrides": {
	  "404": {
		"rewrite": "/index.html",
		"statusCode": 200
	  }
	}
  }
  